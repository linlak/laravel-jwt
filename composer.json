{
    "name": "linlak/laravel-jwt",
    "minimum-stability": "dev",
    "description": "Laravel JWT Library",
    "homepage": "https://github.com/linlak/laravel-jwt",
    "support": {
        "issues": "https://github.com/linlak/laravel-jwt/issues",
        "source": "https://github.com/linlak/laravel-jwt"
    },
    "license": "MIT",
    "keywords": [
        "api",
        "laravel-jwt",
        "laravel auth",
        "jwt",
        "api auth"
    ],
    "authors": [{
        "name": "Linus Nowomukama",
        "email": "nowomukama.linlak.linus2@gmail.com",
        "role": "Developer"
    }],
    "type": "library",
    "prefer-stable": true,
    "config": {
        "sort-packages": true,
        "preferred-install": "dist"
    },
    "require": {
        "php": ">=7.2",
        "lcobucci/jwt": "^3.3",
        "illuminate/support": "5.7.* || 5.8.* || ^6.0",
        "illuminate/http": "5.7.* || 5.8.* || ^6.0",
        "illuminate/contracts": "5.7.* || 5.8.* || ^6.0",
        "illuminate/auth": "5.7.* || 5.8.* || ^6.0"
    },
    "require-dev": {
        "illuminate/console": "5.7.* || 5.8.* || ^6.0"
    },
    "autoload": {
        "psr-4": {
            "Linlak\\Jwt\\": "src/"
        }
    },
    "suggest": {
        "ramsey/uuid": "Required to perform string manulaption such as genarating RFC 4122 version 1,3,4 and 5 univasary unique identifiers (UUID)."
    },
    "extra": {
        "laravel": {
            "providers": [
                "Linlak\\Jwt\\Providers\\JwtServiceProvider"
            ],
            "aliases": {
                "LinJwt": "Linlak\\Jwt\\Facades\\LinJwt"
            }
        },
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
